import { Store, DeviceType } from "../store.slint";
import { DesktopPanel } from "desktop/panel.slint";
import { MobilePanel  } from "mobile/panel.slint";

export component Panel inherits Rectangle {
    if Store.device-type == DeviceType.Desktop: DesktopPanel {}

    if Store.device-type == DeviceType.Mobile: MobilePanel {}
}
